#include "Encoding.h"

std::string Encoding::RandomData(){

	return "base encoding";
}

std::string BinaryEncoding::RandomData(){

	return "binary encoding";
}

std::string ValueEncoding::RandomData(){

	int nLow = 0;
	int nHigh = 100;

	int data = (rand() % (nHigh - nLow + 1)) + nLow;

	   stringstream ss;//create a stringstream
	   ss << data;//add number to the stream
	   return ss.str();//return a string with the contents of the stream

}

std::string TreeEncoding::RandomData(){

	return "tree encoding";
}
