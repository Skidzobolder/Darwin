#ifndef ROSETTA_H_
#define ROSETTA_H_

#include "Globals.h"


class Rosetta
{
private:
	GenomeType m_eGenomeType;

public:
    Rosetta(GenomeType eGenomeType) // default constructor
    {
    	m_eGenomeType = eGenomeType;
    }

    void ModelToGenome();
    void GenomeToModel();

    void randomGenome();
    void initGenome();


    GenomeType GetGenomeType() { return m_eGenomeType; }


};



#endif /* ROSETTA_H_ */
